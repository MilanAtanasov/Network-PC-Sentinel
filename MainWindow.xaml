<Window x:Class="Network_PC_Sentinel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Network_PC_Sentinel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"Width="575">
    <Grid>
        <ComboBox x:Name="cbRechner" HorizontalAlignment="Left" Margin="111,27,0,0" VerticalAlignment="Top" Width="232"/>
        <Label x:Name="lRechner" Content="Rechner" HorizontalAlignment="Left" Margin="10,25,0,0" VerticalAlignment="Top" Width="91"/>
        <TabControl x:Name="tabControl" Margin="10,73,10,10">
            <TabItem Header="Software">
                <Grid Background="#FFE5E5E5">
                    <ListBox x:Name="listBoxSoftware" Margin="10,10,10,10"/>
                </Grid>
            </TabItem>
            <TabItem Header="IPs    ">
                <Grid Background="#FFE5E5E5">
                    <ListBox x:Name="listBoxIPs" Margin="10,10,10,10"/>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
